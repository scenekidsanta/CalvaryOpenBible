</head>

    <body style="padding-top: 390px;">
        <div class="jumbotron" style="position: fixed; width: 100%; top: 0; height: 250px;">
            <h1>TestJumbotron</h1>
            <p class="lead">Filler Filler Filler Filler Filler Filler Filler Filler Filler Filler Filler</p>
            <p><a href="" class="btn btn-primary btn-lg"> Filler Filler Filler Filler</a></p>
        </div>
        <div style="background-image =""">
            <div class="navbar navbar-inverse main-nav">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    </div>
                    <div id="menu" class="navbar-collapse collapse">
                        <ul id="nav" class="nav navbar-nav">
                            <li>@Html.ActionLink("Home", "Index", "Home")
                                <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px;">
                                    <li><a href="#">Misc 1.1</a></li>
                                    <li><a href="#">Misc 1.2</a></li>
                                    <li><a href="#">Misc 1.3</a></li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("About", "About", "Home")
                                <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px;">
                                    <li><a href="#">Misc 2.1</a></li>
                                    <li><a href="#">Misc 2.2</a></li>
                                    <li><a href="#">Misc 2.3</a></li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")
                                <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px;">
                                    <li><a href="#">Misc 3.1</a></li>
                                    <li><a href="#">Misc 3.2</a></li>
                                    <li><a href="#">Misc 3.3</a></li>
                                </ul>
                            </li>
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>
                </div>
            </div>
            <div style="position:absolute; padding-top: 390px; width: 25%; background-color: #FFFFFF; float: left;">
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
            </div>
            <div style="position:absolute; padding-top: 390px; width: 25%; background-color: #FFFFFF; float: left; padding-left: 75%;">
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
            </div>
            <div class="container body-content main">
                @RenderBody()
                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                </footer>
            </div>

            @Scripts.Render("~/bundles/jquery")
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)
        </div>
    </body>


</html>

